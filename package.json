    "@angular/language-service": "13.0.2",
    "@angularclass/hmr": "3.0.0",
    "@types/datatables.net": "^1.10.23",
    "@types/jasmine": "3.6.3",
    "@types/jquery": "^3.5.14",
    "@types/node": "12.11.1",
    "codelyzer": "6.0.1",
